@ExtendWith(#[
	typeof(ContextInitExtension),
	typeof(JavaVersionCheckExtension),
	typeof(PropertyRestoreExtension)
])
@DisplayName("unit: TimeConfig test")
@Tag("unit")
@Tag("janus")
@Tag("sre-unit")
@Tag("sre-simulation")
class ProbesConfigTest {

	@Nullable
	var config : ProbesConfig
	
	@BeforeEach
	def setUp : void {
		this.config = new ProbesConfig
	}
	
	@Test
	@DisplayName("getFolderName")
	def getFolderName : void {
		ProbesConfig::FOLDER_VALUE.assertEquals(this.config.folderName)
	}

	@Test
	@DisplayName("getProbeFolder")
	def getProbeFolder : void {
		var file = this.config.probeFolder
		file.assertNotNull
		ProbesConfig::FOLDER_VALUE.assertEquals(file.name)
	}

	@Test
	@DisplayName("setFolderName")
	def setFolderName : void {
		this.config.folderName = "xyz"
		"xyz".assertEquals(this.config.folderName)
	}

	@Test
	@DisplayName("getProbes")
	def getProbes : void {
