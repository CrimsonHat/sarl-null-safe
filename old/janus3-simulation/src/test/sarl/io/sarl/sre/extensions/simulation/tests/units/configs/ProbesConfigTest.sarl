	}

	@Test
	@DisplayName("getProbe(null)")
	def getProbe_null : void {
		throw new UnsupportedOperationException
	}

	@Test
	@DisplayName("getProbe(UUID) with declared probe")
	def getProbe_UUID_exist : void {
		throw new UnsupportedOperationException
	}

	@Test
	@DisplayName("getProbe(UUID) with not-declared probe")
