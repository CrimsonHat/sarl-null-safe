package io.sarl.sre.tests.runtime.bugs.bug976.mocks

import io.sarl.core.DefaultContextInteractions
import io.sarl.core.Schedules
import io.sarl.sre.tests.testutils.agents.TestingAgent

agent ScalableAgent extends TestingAgent {
	
	uses Schedules, DefaultContextInteractions

	def runAgentTest : RunPolicy {
		every(1000)[emit(new Taskgo)]
		return RunPolicy::STANDARD
	}

	on Taskgo {
		emit(new Back)
	}
	
}
